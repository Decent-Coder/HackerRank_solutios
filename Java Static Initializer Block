        private static boolean flag;

static
{
    Scanner sc=new Scanner(System.in);
    B=sc.nextInt();
    H=sc.nextInt();
    flag=   (H>0&&B>0);

    if(!flag)
        System.out.println("java.lang.Exception: Breadth and height must be positive");

