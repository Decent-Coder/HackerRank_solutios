

public class Solution {



    //private static final Scanner scanner = new Scanner(System.in);

    public static void main(String[] args) {
        int N = new java.util.Scanner(System.in).nextInt();
        for (int i = 1;i<11;i++)
        {
            System.out.println(N+" x "+i+" = "+(N*i));
        }

    }
}
